{
  "compilerOptions": {
    "module": "ESNext",
    "jsx": "preserve",
    "noLib": true,
    "noEmit": false,
    "removeComments": true,
    "useDefineForClassFields": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "include": [
    "env.d.ts",
    "index.ts",
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "function"
  ],
  "typedocOptions": {
    "entryPoints": [
      "index.ts"
    ],
    "out": "docs"
  }
}